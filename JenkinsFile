pipeline {
  agent any
  tools {
    jdk 'Java'
    maven 'Maven'
  }
  stages {
    stage("Checkout from SCM") {
      steps {
        git branch: 'main', credentialsId: 'GitHubcredentials', url: 'https://github.com/rakshandhak/spring-boot-microservice.git'
      }
    }
    stage("Build Application") {
      steps {
        sh "mvn clean install"
      }
    }

  }
}
